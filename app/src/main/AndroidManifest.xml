<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lostdotcom.notifind">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReportViewingActivity"></activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".NotificationSettingsActivity" />
        <activity android:name=".PrivacySettingsActivity" />
        <activity android:name=".AccountSettingsActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ReportCreation" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>